generated":{"line":251,"column":0}},{"source":"static/css/form-style.css","original":{"line":252,"column":0},"generated":{"line":252,"column":0}}],"sources":{"static/css/form-style.css":"@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700;800&display=swap\");\n\n\n:root{\n  --mainColour: #212529;\n  --otherColor: #450920;\n  --royal-purple: #7209b7;\n  --lighter-purple: #7209b7;\n  --white: #fff;\n}\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Poppins\", sans-serif;\n}\n\n.container {\n  width: 100%;\n  background-color: var(--white);\n  min-height: 100vh;\n}\n\n.response{\n  background-color: rgb(15, 215, 15);\n  /* background-color: rgb(231, 44, 15); */\n  height: fit-content;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: fit-content;\n  padding: .7rem 5rem;\n  flex-wrap: wrap;\n  margin: .3rem auto;\n  border-radius: 10px;\n  visibility: hidden;\n  display: none;\n}\n\n.response label{\n  color: #fff;\n  font-weight: 700;\n}\n\nform {\n  display: flex;\n  flex-direction: column;\n  margin: 1rem auto;\n  align-items: center;\n  justify-content: center;\n  padding: 1rem 5rem;\n  transition: all 0.2s 0.7s;\n}\n\n.title {\n  font-size: 2.2rem;\n  color: #444;\n  margin-bottom: 10px;\n}\n\n.form-group{\n  font-size: 25px;\n  font-weight: bold;\n  width: 400px;\n  max-width: 45%;\n  display: flex;\n  flex-direction: column;\n  /* line-height: 2rem; */\n  gap: .2rem;\n}\n\n.form-group label{\n  font-size: medium;\n}\n\n.form-group input{\n  height: 2.5rem;\n}\n\n.form-group.success .form-control{\n  border-color: #09c372;\n}\n\n.form-group.error .form-control{\n  border-color: #ff3860;\n}\n\n.form-group .error{\n  color: #ff3860;\n  font-weight: normal;\n  font-size: small;\n}\n\n.form-group .form-control:focus{\n  /* helps prevent overlap with error and succes outline */\n  outline: 0;\n}\n.form-control{\n  border: 2px solid #665665;\n  padding: .4rem;\n  font-size: large;\nborder-radius: 17px;\nheight: 40px;\nfont-weight: 500;\n  width: 100%;\n}\n\n.form-input i {\n  text-align: center;\n  line-height: 55px;\n  /* color: #acacac; */\n  color: #655959;\n  transition: 0.5s;\n  font-size: 1.1rem;\n}\n\n.form-input input {\n  background: none;\n  outline: none;\n  border: none;\n  line-height: 1;\n  font-weight: 600;\n  font-size: 1.1rem;\n  /* color: #333; */\n  color: #655959;\n}\n\n.form-input input::placeholder {\n  color: #aaa;\n  font-weight: 500;\n}\n\n.social-text {\n  padding: 0.7rem 0;\n  font-size: 1rem;\n}\n\n.social-media {\n  display: flex;\n  justify-content: center;\n}\n\n.social-icon {\n  height: 46px;\n  width: 46px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  margin: 0 0.45rem;\n  color: #333;\n  border-radius: 50%;\n  border: 1px solid #333;\n  text-decoration: none;\n  font-size: 1.1rem;\n  transition: 0.3s;\n}\n\n.social-icon:hover {\n  color: var(--lighter-purple);\n  border-color: var(--royal-purple);\n}\n\n.btn {\n  width: 150px;\n  background-color: var(--white);\n  border: 2px solid var(--lighter-purple);\n  outline: none;\n  height: 49px;\n  border-radius: 49px;\n  color: var(--lighter-purple);\n  text-transform: uppercase;\n  font-weight: 600;\n  margin: 10px 0;\n  cursor: pointer;\n  transition: 0.5s;\n}\n\n.btn:hover {\n  background-color: var(--royal-purple);\n  color: var(--white);\n}\n\n\n\n.btn.transparent {\n  margin: 0;\n  background: none;\n  border: 2px solid #fff;\n  width: 130px;\n  height: 41px;\n  font-weight: 600;\n  font-size: 0.8rem;\n}\n\n.signup-link{\n  font-size: smaller;\n  margin-top: .4rem;\n}\n.signup-link p a{\n\ttext-decoration: none;\n\tfont-weight: 700;\n  color: var(--lighter-purple);\n}\n\n.signup-link p a:hover{\n\ttext-decoration: underline;\n}\n\n\n\n@media (max-width: 870px) {\n  .container {\n    min-height: 800px;\n    height: 100vh;\n  }\n  .signin-signup {\n    width: 100%;\n    top: 95%;\n    transform: translate(-50%, -100%);\n    transition: 1s 0.8s ease-in-out;\n  }\n\n  .image {\n    width: 200px;\n    transition: transform 0.9s ease-in-out;\n    transition-delay: 0.6s;\n  }\n\n  .btn.transparent {\n    width: 110px;\n    height: 35px;\n    font-size: 0.7rem;\n  }\n\n}\n\n@media (max-width: 570px) {\n  form {\n    padding: 0 1.5rem;\n  }\n\n  .image {\n    display: none;\n  }\n  .container {\n    padding: 1.5rem;\n  }\n\n}\n\n"},"lineCount":252}},"error":null,"hash":"31ac7603662f9a0391651fac01b73328","cacheData":{"env":{}}}